# INI File format

[Time]
command=date +%R
interval=5
color=#9b8d7f

# This shows only the percentage
[battery]
helpScript=sh -c "scripts/battery"
command=$helpScript | grep -m 1 "%" | rev | cut -d ' ' -f1 | rev
interval=10
color=#9b8d7f

[weather]
helpScript1=sh -c "scripts/weather"
command=$helpScript1
interval=10
color=#9b8d7f


