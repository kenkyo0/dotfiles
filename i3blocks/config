# INI File format
# ! make sure you've exported the .scripts in your used shell

[Time]
command=date +%R
interval=5
color=#9b8d7f

# This shows only the percentage
[battery]
command=~/.config/.scripts/battery | grep -m 1 "%" | rev | cut -d ' ' -f1 | rev
interval=10
color=#9b8d7f

[weather]
command=~/.config/.scripts/weather
interval=10
color=#9b8d7f

# <-- new block here 

[filler]
command=echo '~/\\_/\\~      |'
interval=1
color=#9b8d7f
